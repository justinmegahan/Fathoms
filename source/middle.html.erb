		<title><%= schoolData[:name] %></title>
    
		<link href="../../../stylesheets/normalize.css" media="screen" rel="stylesheet" type="text/css" />
		<link href="../../../stylesheets/all.css" media="screen" rel="stylesheet" type="text/css" />
		<link href="../../../stylesheets/foundation.min.css" media="screen" rel="stylesheet" type="text/css" />

        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    	<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    	<script src="../../../javascripts/Chart.js" type="text/javascript"></script>
		<script src="../../../javascripts/all.js" type="text/javascript"></script>
		<script src="../../../javascripts/foundation.min.js" type="text/javascript"></script>

  </head>

  
	<body>

		<h1><%= schoolData[:name] %></h1>

		<div class="row">

			<ul class="breadcrumbs small-11 small-centered medium-10 medium-centered large-10 large-centered columns">
				<li><a href="../../../index.html">Home</a></li>
				<li><a href="../../<%= countyData[:slug] %>.html"><%= countyData[:name] %></a></li>
				<li><a href="../<%= districtData[:slug] %>.html"><%= districtData[:name] %></a></li>
				<li class="current"><a href="#"><%= schoolData[:name] %></a></li>
			</ul>

		</div>

		<div class="row centered">


			<script>



			      window.onload = function(){
			        var ctz = document.getElementById("canvas2").getContext("2d");
					window.myLine = new Chart(ctz).Line(lineChartData, {
						responsive: true,
						scaleShowGridLines : false
						});
			        };

			</script>

		</div>

	<div class="row">
			<div style="width:30%">
			<div>
				<canvas id="canvas2" height="450" width="600"></canvas>
			</div>
		</div>

 <script>
		var randomScalingFactor = function(){ return Math.round(Math.random()*100)};
		var lineChartData = {
			labels : ["2011","2012","2013"],
			datasets : [
				{
					label: "School",
					fillColor : "rgba(220,220,220,0.2)",
					strokeColor : "rgba(220,220,220,1)",
					pointColor : "rgba(220,220,220,1)",
					pointStrokeColor : "#fff",
					pointHighlightFill : "#fff",
					pointHighlightStroke : "rgba(220,220,220,1)",
					data : [0,<%= schoolData[:api13] %>,<%= schoolData[:api12] %>]
				},
				{
					label: "State Average",
					fillColor : "rgba(151,187,205,0.2)",
					strokeColor : "rgba(151,187,205,1)",
					pointColor : "rgba(151,187,205,1)",
					pointStrokeColor : "#fff",
					pointHighlightFill : "#fff",
					pointHighlightStroke : "rgba(151,187,205,1)",
					data : [0,300,400]
				}
			]

		}

	</script>

		<div class="row">

			<table class="small-centered small-12 large-12 large-centered columns">
				<tbody>
					<tr><td>State Rank</td><td><%= schoolData[:stateRank] %></td></tr>
					<tr><td>Class Size</td><td><%= schoolData[:classSize] %></td></tr>
					<tr><td>enrollment</td><td><%= schoolData[:enrollment] %></td></tr>
					<tr><td>street</td><td><%= schoolData[:street] %></td></tr>
					<tr><td>city</td><td><%= schoolData[:city] %></td></tr>
					<tr><td>zip</td><td><%= schoolData[:zip] %></td></tr>
					<tr><td>state</td><td><%= schoolData[:state] %></td></tr>
				</tbody>
			</table>
		</div>

		<div class="row">

			<iframe class="small-12 small-centered large-centered columns" width="600" height="450" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/view?key=AIzaSyC_ixNhdFtsTVSGcE0Yt4fZ_sCOvEUcGFE&center=<%= schoolData[:latitude] %>,<%= schoolData[:longitude] %>&zoom=15"></iframe>
		</div>