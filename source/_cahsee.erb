							$i = 0
							$j = 0
							$html = ''
							fullIcon = (score/10).floor
							clearIcon = 10-fullIcon
							until $i >= fullIcon  do 
								$html = $html + '<img class="infographicIcon small-2" src="../../../images/book.png">'
								$i +=1
							end
							until $j >= clearIcon  do
								$html = $html + '<img class="infographicIcon small-2 opacity" src="../../../images/book.png">'
								$j +=1
							end
							$html
					end %>	
