		<title><%= schoolData[:name] %></title>
    
		<link href="../../stylesheets/normalize.css" media="screen" rel="stylesheet" type="text/css" />
		<link href="../../stylesheets/all.css" media="screen" rel="stylesheet" type="text/css" />
		<link href="../../stylesheets/foundation.min.css" media="screen" rel="stylesheet" type="text/css" />
    
		<script src="../../javascripts/all.js" type="text/javascript"></script>
		<script src="../../javascripts/foundation.min.js" type="text/javascript"></script>

  </head>
  
	<body class="row">

		<h1><%= schoolData[:name] %></h1>

		<ul class="breadcrumbs small-6 large-centered columns">
			<li><a href="../../index.html">Home</a></li>
			<li><a href="../../<%= county %>.html"><%= county %></a></li>
			<li><a href="../<%= district %>.html"><%= districtData[:name] %></a></li>
			<li class="current"><a href="#"><%= schoolData[:name] %></a></li>
		</ul>

		<table class="small-6 large-centered columns">
			<tbody>
				<tr><td>State Rank</td><td><%= schoolData[:stateRank] %></td></tr>
				<tr><td>2013 API</td><td><%= schoolData[:api13] %></td></tr>
				<tr><td>2012 API</td><td><%= schoolData[:api12] %></td></tr>
				<tr><td>Class Size</td><td><%= schoolData[:classSize] %></td></tr>
				<tr><td>enrollment</td><td><%= schoolData[:enrollment] %></td></tr>
				<tr><td>graduationRate</td><td><%= schoolData[:graduationRate] %></td></tr>
				<tr><td>latitude</td><td><%= schoolData[:latitude] %></td></tr>
				<tr><td>longitude</td><td><%= schoolData[:longitude] %></td></tr>
				<tr><td>street</td><td><%= schoolData[:street] %></td></tr>
				<tr><td>city</td><td><%= schoolData[:city] %></td></tr>
				<tr><td>zip</td><td><%= schoolData[:zip] %></td></tr>
				<tr><td>state</td><td><%= schoolData[:state] %></td></tr>
			</tbody>
		<table>

		<iframe class="small-6 large-centered columns" width="600" height="450" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/view?key=AIzaSyC_ixNhdFtsTVSGcE0Yt4fZ_sCOvEUcGFE&center=<%= schoolData[:latitude] %>,<%= schoolData[:longitude] %>&zoom=15"></iframe>
