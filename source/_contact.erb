		<div class="row">
			<div class="small-12 medium-6 columns">
				<div class="row">
					<div class="small-12 columns"><h2>Contact</h2></div>
				</div>
				
				<%= if defined?(schoolData[:website]) && (schoolData[:website] != '0') && (schoolData[:website] != '') then '<div class="row contact"> <img class="small-2 columns" src="../../../images/website.png"> <div class="small-10 columns"> <p class="scoreDetailsSmall"><a href="http://' + schoolData[:website] +'">' + schoolData[:website] + '</a></p> </div> </div>' end %>


				<div class="row contact">
					<img class="small-2 columns" src="../../../images/address.png">
					<div class="small-10 columns">
						<p class="scoreDetailsSmall"><%= schoolData[:street] %><br><%= schoolData[:city] %>, <%= schoolData[:state] %> <%= schoolData[:zip] %></p>
					</div>
				</div>
				<div class="row contact">
					<img class="small-2 columns" src="../../../images/phone.png">
					<div class="small-10 columns">
						<p class="scoreDetailsSmall"><%= schoolData[:phone] %></p>
					</div>
				</div>
			</div>
			<div id ="map"class="small-12 medium-6 columns flex-video">
				<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC_ixNhdFtsTVSGcE0Yt4fZ_sCOvEUcGFE"></script>
			    <script>
					var map;
					function initialize() {
						var mapOptions = {
						zoom: 16,
						center: new google.maps.LatLng(<%= schoolData[:latitude] %>,<%= schoolData[:longitude] %>)
						};

						map = new google.maps.Map(document.getElementById('map'),
					      mapOptions);
					  
	  					var myLatLng = new google.maps.LatLng(<%= schoolData[:latitude] %>,<%= schoolData[:longitude] %>);
						var marker = new google.maps.Marker({
      					position: new google.maps.LatLng(<%= schoolData[:latitude] %>,<%= schoolData[:longitude] %>),
      					map: map,
      					title: 'Hello World!'
  					});
					}

					google.maps.event.addDomListener(window, 'load', initialize);
			    </script>
			</div>
		</div>